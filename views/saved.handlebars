<div id="navBar">   
    <a class="navbarItem" href="/">Home</a>
    <a class="navbarItem" href="/saved">Saved</a>
    <button class="navbarItem navButton" id="scrapeButton">Scrape New Articles</button>
    <button class="navbarItem navButton" id="clear">Clear all articles</button>
</div>

<div>
    <h1 id="Saved-Header">NYTIMES Scraper</h1>
</div>

<div class="articleList">
    {{#each article}}
    <div class="articleDiv">
        <div class="headline">
            <h2>{{title}}</h2>
            <button id="deleteSavedButton">Clear</button>
        </div>
        <div class="link">
            <h3><a href="http://www.nytimes.com/{{link}}">{{link}}</a></h2>
        </div>
        
    </div>
    {{/each}}

</div>
